<main id="main" class="main">
    <div class="container">
        <div class="card">
            <div class="card-body">
                <h5 class="card-title">Add Quiz wise Question </h5>
                <form class="row g-3">
                    <div class="col-12">
                        <label for="inputQuestions4" class="form-label">
                            <span class="text-danger">*</span>
                            User ID
                        </label>
                        @* <input type="number" class="form-control" id="inputQuestions4"> *@
                        <select id="inputState" class="form-select">
                            <option selected disabled>User ID</option>
                        </select>
                    </div>
                    <div class="col-12">
                        <label for="inputQuestions4" class="form-label">
                            <span class="text-danger">*</span>
                            Question Level
                        </label>
                        @* <input type="number" class="form-control" id="inputQuestions4"> *@
                        <select id="inputState" class="form-select">
                            <option selected disabled>Levels</option>
                            <option>Hard</option>
                            <option>Medium</option>
                            <option>Easy</option>
                        </select>
                    </div>   
                    <div class="text-center">
                        <button type="submit" class="btn btn-primary">Save</button>
                        <a class="btn btn-secondary" asp-controller="QuestionLevel" asp-action="List">Cancel</a>
                    </div>
                </form><!-- Vertical Form -->
            </div>
        </div>
    </div>
</main>
